		},
		{
			"Name": "IsCold",
			"Code": "(ColdBiomes/Scanner.BiomeSize >= 0.60)"
		},
		{
			"Name": "IsHell",
			"Code": "(HellBiome/Scanner.BiomeSize >= 0.60)"
		},
		{
			"Name": "IsEnd",
			"Code": "(EndBiome/Scanner.BiomeSize >= 0.60)"
		},
		{
			"Name": "IsForest",
			"Code": "((#Trees# /*&& Scanner.AverageTemperature >= 0.28*/ && Scanner.AverageTemperature <= 0.78) && !(#IsJungle# || #IsConiferous# || #IsSwamp#))"
		},
		{
			"Name": "IsPlains",
			"Code": "Scanner.AverageTemperature >= 0.77 && Scanner.AverageTemperature <= 0.83 && #BugPlants# && (FoliageBlocks/Scanner.BlockSize) < 0.01"
		}
	]
